{
  "name": "grapesjs-mjml",
  "version": "0.1.3",
  "description": "MJML Componenets integration in GrapesJS",
  "main": "dist/grapesjs-mjml.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "npm run v:patch && webpack --env.production",
    "start": "webpack-dev-server --open --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artf/grapesjs-mjml.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "mjml",
    "wysiwyg"
  ],
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "grapesjs": "^0.x"
  },
  "dependencies": {
    "mjml4-in-browser": "1.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.1.0",
    "html-webpack-plugin": "^3.2.0",
    "install-peers": "^1.0.3",
    "node-sass": "^4.12.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
